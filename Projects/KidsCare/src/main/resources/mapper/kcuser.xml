<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.kcuser">
	 <select id="list" >
		SELECT *
		FROM kcuser
	</select>
	
	<insert id="add">
		INSERT into kcuser
		VALUES (#{userId},#{name},#{phone},#{email},#{role})
	</insert>
	
	<update id="update">
		UPDATE kcuser
		SET name=#{name},phone=#{phone},#{email},#{role}
		WHERE userId=#{userId}
	</update>
	
</mapper>