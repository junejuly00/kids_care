<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.userreview">
	<select id="list">
		SELECT *
		FROM userreview
	</select>
	
	<insert id="add">
		INSERT into userreview
		VALUES (#{username})
	</insert>
</mapper>